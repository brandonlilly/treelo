<div class="session-container">

  <div class="session-panel">
    <h1>Log in to Trello Clone</h1>

    <form action="/session" method="post">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <label>Email<br>
        <input name="user[email]" type="text"><br>
      </label><br>
      <label>Password<br>
        <input name="user[password]" type="password"><br>
      </label><br>
      <button class="btn btn-primary">Log in</button><br>
    </form>
    Or...
    <br><br>
    <button class="btn btn-primary guest-login">Log in as guest</button><br>
  </div>

</div>

<script type="text/javascript">
  $('.guest-login').click(function (event) {
    console.log('clicked guest');

  });
</script>
